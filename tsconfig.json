{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"rootDir": "./framework/",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,

		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,

		"esModuleInterop": true
	}
}
